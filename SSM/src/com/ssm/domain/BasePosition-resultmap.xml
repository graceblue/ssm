<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BasePosition">
  <resultMap type="com.ssm.domain.BasePosition" id="BasePositionResultMap">
    <id column="POSITION_ID" property="positionId" />
    <result column="POSITION_CODE" property="positionCode" />
    <result column="POSITION_NAME" property="positionName" />
    <result column="POSITION_FULL_NAME" property="positionFullName" />
    <result column="POSITION_TYPE_CD" property="positionTypeCd" />
    <result column="POSITION_DESC" property="positionDesc" />
    <result column="IS_DELETE" property="isDelete" />
    <result column="ORG_ID" property="org" />
	<result property="createTime" column="CREATE_DATE" javaType="Date" jdbcType="DATE"/>
    <result column="NAME" property="positionTypeCdName" />
    <result column="USER_NAME" property="createOpName" />
    <result column="ORG_NAME" property="orgName" />
    <result column="AREA_NAME" property="areaName" />
  </resultMap>
</mapper>
