<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BaseUser">
	<resultMap type="com.ssm.domain.BaseUser" id="BaseUserResultMap">
		<id property="userId" column="user_id"/>
		<result property="userName" column="user_name"/>
		<result property="accountName" column="account_name"/>
		<result property="accountPasswordCd" column="account_password_cd"/>
		<result property="userStateCd" column="user_state_cd"/>
		<result property="showOrderNo" column="show_order_no"/>
		<result property="isDelete" column="is_delete"/>
		<result property="identityId" column="identity_id"/>
		<result property="gender" column="gender"/>
		<result property="age" column="age"/>
		<result property="phoneNumber" column="phone_number"/>
		<result property="email" column="email"/>
		<result property="description" column="description"/>
		<result property="org" column="org_id"  jdbcType="VARCHAR" />
		<result property="createTime" column="CREATE_DATE" javaType="Date" jdbcType="DATE"/>
		<result property="orgName" column="ORG_NAME"/>
		<result property="areaName" column="AREA_NAME"/>
		<result property="areaId" column="AREA_ID"/>
		<result property="protect" column="protect"/>
		<result property="major" column="major"/>
		<result property="roler" column="roler"/>
		<result property="produceScene" column="produce_scene"/>
		<result property="manageScene" column="manage_scene"/>
		<result property="nextUpdateTime" column="NEXT_UPDATE_TIME"/>
	</resultMap>
</mapper>
